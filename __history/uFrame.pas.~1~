unit uFrame;

interface

uses
  uComponente;

type

  TFrame = class(TComponente)
  public
    constructor create(aX, aY : Integer);
  end;

implementation

constructor TFrame.create(aX, aY : Integer);
begin
  self.X := aX;
  self.Y := aY;
  //self.Componentes :=  TComponente as [TFrame];
  self.Altura  := 200;
  self.Largura := 200;
end;

end.

